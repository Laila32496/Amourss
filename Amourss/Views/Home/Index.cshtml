        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <header class="page-header section-header clearfix">
                        <h2><strong>FEATURED WORK</strong></h2>
                    </header>
                    <div class="article-list row row2" style="position:relative">
                        @foreach (var element in Model.FrontCauses)
                        {
                            <div class="items zoom col-xs-12 col-sm-4 charity-latest-news-home-one  charity-latest-news-shop">
                                <a href="#/" class="img-thumb">
                                    <figure>
                                        <img src="@(string.IsNullOrWhiteSpace(element.ImagePath)? "" : Url.Content(element.ImagePath))" class="attachment-charity_causes_thumb wp-post-image" alt="blog-pic" width="600" height="400">
                                    </figure>
                                </a>
                                <h3><a href="@Url.Action("Index","Home")">@element.Title</a></h3>
                                <span class="date">@element.TimeStamp.ToString("dd MMM yyyy")</span>
                                
                                <p class="donation-summary">@element.Description</p>
                                <br />
                                <a class="more-link btn btn-default btn-bottom" href="@Url.Action("Project","Home")">READ MORE</a>
                     
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </section>



</div>